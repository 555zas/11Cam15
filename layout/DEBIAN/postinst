#!/bin/bash
//iPhone 7+
if [ -d "/System/Library/PrivateFramework/CMCaptureFramework.framework/D111" ] 
then
    echo "D111 exists - iPhone 7+ Detected." 
    
cp -R /System/Library/PrivateFramework/CMCaptureFramework.framework/D111/* /Library/Application Support/11Cam15/7PlusBAK/
rm -rf /System/Library/PrivateFramework/CMCaptureFramework.framework/D111/*

cp -R /Library/Application Support/11Cam15/D111/* /System/Library/PrivateFramework/CMCaptureFramework.framework/D111/

else
    echo "Error: D111 does not exists."
fi
//iPhone 8+
if [ -d "/System/Library/PrivateFramework/CMCaptureFramework.framework/D211" ] 
then
    echo "D211 exists - iPhone 8+ Detected." 
    
cp -R /System/Library/PrivateFramework/CMCaptureFramework.framework/D211/* /Library/Application Support/11Cam15/8PlusBAK/
rm -rf /System/Library/PrivateFramework/CMCaptureFramework.framework/D211/*

cp -R /Library/Application Support/11Cam15/D211/* /System/Library/PrivateFramework/CMCaptureFramework.framework/D111/

else
    echo "Error: D211 does not exists."
fi
